USE DATABASE [master];
@tbl =
	EXTRACT
		col1 int,
		col2 float,
		col3 decimal,
		col4 byte[]
	FROM "/BIT_CRM/input.txt"
	USING new MyCustomExtractors.FixedWidthExtractor (
		new SQL.MAP<string, string> {
			{"col1","3"},
			{"col2","2"},
			{"col3","1"},
			{"col4","2"}
		}
	);
OUTPUT @tbl
TO "/BIT_CRM/parsed/output.csv"
USING Outputters.Csv();